
#Server options
[server]
	#Specifies if a warning should be printed out to server operators if flying is disabled.
	#Default: true
	flyingEnabledWarning = true

	#Playing dead
	[server.playing_dead]
		revivalPlayerProtection = 4

	[server.misc]
		resummonedPhase = 5
		rotationSpeed = 0.3
		endOfPhaseFiveBombableExclusively = true

	[server.roaring]
		minimumRoarInterval = 10
		maximumRoarInterval = 20

	[server.flaming_skulls]
		flamingSkullExplosionSize = 7.0
		flamingSkullSpeedModifier = 1.2

